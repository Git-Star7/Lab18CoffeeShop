
@{
    ViewData["Title"] = "DisplayUserInfo";
}

<h1>DisplayUserInfo</h1>


<table class="col-md-8">
    <tr>
        <th>User Name</th>
        <th>Email</th>
        <th>Password</th>
        <th>Gender</th>
        <th>Phone Number</th>
        <th>Age</th>
    </tr>
    <tr>
        <td>@TempData["BagName"].ToString()</td>
        <td>@TempData["BagEmail"].ToString()</td>
        <td>@TempData["BagPassword"].ToString()</td>
        <td>@TempData["BagGender"].ToString()</td>
        <td>@TempData["BagPhone"].ToString()</td>
        <td>@TempData["BagAge"].ToString()</td>
    </tr>
</table>